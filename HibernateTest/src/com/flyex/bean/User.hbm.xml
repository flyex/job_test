<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.flyex.bean.User" table="user">
        <composite-id>
            <key-property name="id" type="java.lang.Integer" column="Id" />
            <key-property name="userName" type="java.lang.String" column="UserName" />
            <key-property name="age" type="java.lang.Integer" column="Age" />
        </composite-id>
    </class>
</hibernate-mapping>